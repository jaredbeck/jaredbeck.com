!!!
%html
  %head
    %title Jared Beck
    = stylesheet_link_tag :all
    = javascript_include_tag :defaults
    = csrf_meta_tag
  %body
    #c1
      %h1 jared beck
      #c2
        .flashes
          %p.notice= notice
          %p.alert= alert
        = yield
      - if user_signed_in?
        %p= link_to "Logout", destroy_user_session_path, :method => :delete
